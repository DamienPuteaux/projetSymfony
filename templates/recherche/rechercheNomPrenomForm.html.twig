{# templates/recherche/rechercheNomPrenomForm.html.twig #}

{% extends 'base.html.twig' %}

{% block title %}Recherche par Nom et Prénom{% endblock %}

{% block body %}
    <h1>Recherche par Nom et Prénom</h1>

    {{ form_start(form) }}
    {{ form_row(form.nom) }}
    {{ form_row(form.prenom) }}
    {{ form_row(form.submit) }} {# Affiche le bouton de soumission #}
    {{ form_end(form) }}
{% endblock %}
